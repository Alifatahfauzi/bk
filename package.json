{
  "name": "@skyzopedia/baileys-pro",
  "version": "8.0.6",
  "description": "Websocket Whatsapp API for Node.js",
  "keywords": [
    "whatsapp",
    "websocket",
    "api",
    "nodejs",
    "baileys",
    "wabot",
    "proto",
    "whatsapp-websocket"
  ],
  "homepage": "https://github.com/skyzopedia/Baileys",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/skyzopedia/Baileys.git"
  },
  "license": "MIT",
  "author": "Skyzopedia",
  "main": "./lib/index.js",
  "files": [
    "lib/*",
    "WAProto/*"
  ],
  "scripts": {
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "example": "node --inspect Example/example.js",
    "lint": "eslint src --ext .js",
    "lint:fix": "yarn lint --fix",
    "release": "release-it",
    "test": "jest"
  },
  "dependencies": {
    "@cacheable/node-cache": "*",
    "@hapi/boom": "*",
    "@whiskeysockets/eslint-config": "github:whiskeysockets/eslint-config",
    "async-mutex": "*",
    "axios": "*",
    "cache-manager": "*",
    "figlet": "*",
    "fs": "*",
    "gradient-string": "2.0.2",
    "libsignal-xeuka": "npm:libsignal-skyzopedia",
    "lodash": "*",
    "lru-cache": "*",
    "moment-timezone": "*",
    "music-metadata": "*",
    "os": "*",
    "p-queue": "*",
    "path": "*",
    "pbjs": "^0.0.14",
    "pino": "*",
    "protobufjs": "^7.5.4",
    "readline": "*",
    "ws": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "@types/ws": "*",
    "cache-manager": "*",
    "conventional-changelog-cli": "*",
    "eslint": "*",
    "jest": "*",
    "json": "*",
    "link-preview-js": "*",
    "open": "*",
    "protobufjs-cli": "*",
    "release-it": "*"
  },
  "peerDependencies": {
    "audio-decode": "*",
    "jimp": ">=0.16.0",
    "link-preview-js": "*"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "bugs": {
    "url": "https://github.com/skyzopedia/Baileys/issues"
  }
